<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyPackSpeech" x:Class="MyPackSpeech.SemesterView" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="69*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0"/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <local:SemesterControl x:Name="sem11" Margin="5,5,5,5" Grid.Column="1" Grid.Row="1"/>
        <local:SemesterControl x:Name="sem12" Margin="5,5,5,5" Grid.Column="2" Grid.Row="1"/>
        <local:SemesterControl x:Name="sem21" Margin="5,5,5,5" Grid.Column="1" Grid.Row="2"/>
        <local:SemesterControl x:Name="sem22" Margin="5,5,5,5" Grid.Column="2" Grid.Row="2"/>
        <local:SemesterControl x:Name="sem31" Margin="5,5,5,5" Grid.Column="1" Grid.Row="3"/>
        <local:SemesterControl x:Name="sem32" Margin="5,5,5,5" Grid.Column="2" Grid.Row="3"/>
        <local:SemesterControl x:Name="sem41" Margin="5,5,5,5" Grid.Column="1" Grid.Row="4"/>
        <local:SemesterControl x:Name="sem42" Margin="5,5,5,5" Grid.Column="2" Grid.Row="4"/>
        

    </Grid>
</UserControl>
